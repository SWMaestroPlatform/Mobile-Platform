<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="soma.iot.sympathyhome" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FamilyPhotoActivity"
            android:label="@string/title_activity_family_photo" >
        </activity>
        <activity
            android:name=".activity.SmartHomeAcitivy"
            android:label="@string/title_activity_smart_home_acitivy" >
        </activity>
    </application>

</manifest>
